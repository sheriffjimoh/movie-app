{
    "private": true,
    "name": "web-movie-apps",
    "description": "my web movie apps",
    "version": "1.0.0",
    "engines": {
        "node": "14.x"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "scss": "node-sass --watch resources/css/ -o public/css",
        "postinstall": "npm run prod"
    },
    "devDependencies": {
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.3.0",
